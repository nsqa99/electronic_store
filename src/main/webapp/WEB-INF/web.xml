<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchServlet</servlet-name>
  	<display-name>SearchServlet</display-name>
  	<description></description>
  	<servlet-class>ltweb.electronic_store.controller.SearchServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminServlet</servlet-name>
  	<display-name>AdminServlet</display-name>
  	<description></description>
  	<servlet-class>ltweb.electronic_store.controller.AdminServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteAdminServlet</servlet-name>
  	<display-name>DeleteAdminServlet</display-name>
  	<description></description>
  	<servlet-class>ltweb.electronic_store.controller.DeleteAdminServlet</servlet-class>
  </servlet>
   <servlet-mapping>
  	<servlet-name>SearchServlet</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminServlet</servlet-name>
  	<url-pattern>/AdminServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteAdminServlet</servlet-name>
  	<url-pattern>/DeleteAdminServlet</url-pattern>
  </servlet-mapping>
</web-app>